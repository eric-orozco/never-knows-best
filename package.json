{
  "name": "Never-Knows-Best",
  "version": "1.0.0",
  "description": "Project seed including Angular, TypeScript, WebPack, MongoDB, GraphQL and more.",
  "main": "server.js",
  "repository": "https://github.com/eric-orozco/never-knows-best",
  "author": "Eric Orozco",
  "scripts": {
    "test": "",
    "build:dev": "webpack-dev-server --inline --progress --host $IP --port $PORT --config webpack.config.dev.js",
    "build:prod": "del-cli dist && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && npm run clean",
    "clean": "del-cli 'src/app/**/*.js' 'src/*.js' 'src/**/*.ngfactory.ts' 'src/app/**/*.js.map' 'src/app/**/*.shim.ts' 'src/**/*.ngsummary.*' 'src/app/**/*.ngstyle.ts' 'dist/app'",
    "serve": "lite-server",
    "c9:dev": "webpack-dev-server --inline --progress --host $C9_IP --port $C9_PORT --public $C9_HOSTNAME --config webpack.config.dev.js",
    "c9:serve:prod": "npm run serve --host $C9_IP --port $C9_PORT --public $C9_HOSTNAME",
    "c9:prod": "npm run build:prod && npm run c9:serve:prod",
    "c9:api:dev": "PORT=8081 HOST=$C9_IP npm run api:dev",
    "api:dev": "nodemon server.js",
    "start": "PORT=8081 node server.js && npm run build:prod && npm run serve"
  },
  "engines": {
    "node": "8.11.2",
    "npm": "6.1.0"
  },
  "dependencies": {
    "@angular/animations": "~6.0.4",
    "@angular/cdk": "~6.2.1",
    "@angular/common": "~6.0.4",
    "@angular/core": "~6.0.4",
    "@angular/forms": "~6.0.4",
    "@angular/http": "~6.0.4",
    "@angular/material": "~6.2.1",
    "@angular/platform-browser": "~6.0.4",
    "@angular/platform-browser-dynamic": "~6.0.4",
    "@angular/router": "~6.0.4",
    "async": "~2.6.1",
    "body-parser": "^1.18.3",
    "core-js": "~2.5.7",
    "express": "~4.16.3",
    "hammerjs": "~2.0.8",
    "mongoose": "^5.1.6",
    "rxjs": "~6.2.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular/compiler": "~6.0.4",
    "@angular/compiler-cli": "~6.0.4",
    "@ngtools/webpack": "~6.0.8",
    "@types/core-js": "~2.5.0",
    "@types/node": "~10.3.2",
    "angular-router-loader": "~0.8.5",
    "angular2-template-loader": "~0.6.2",
    "awesome-typescript-loader": "~5.0.0",
    "compression-webpack-plugin": "~1.1.11",
    "css-loader": "^0.28.11",
    "del-cli": "~1.1.0",
    "file-loader": "^2.0.0",
    "html-loader": "~0.5.5",
    "html-webpack-plugin": "~3.2.0",
    "jsdoc": "~3.5.5",
    "lite-server": "~2.3.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "~1.17.5",
    "raw-loader": "~0.5.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "typescript": "2.7.2",
    "url-loader": "^1.1.1",
    "webpack": "~4.12.0",
    "webpack-bundle-analyzer": "~2.13.1",
    "webpack-cli": "~3.0.4",
    "webpack-dev-server": "~3.1.4",
    "webpack-merge": "~4.1.2"
  },
  "license": "UNLICENSED"
}
